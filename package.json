    "vscode:prepublish": "npm run compile",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "compile": "tsc -p ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.0.0",
    "typescript": "^5.9.3"
  }
