{
  "name": "mkdocs-vscode",
  "displayName": "MkDocs Preview",
  "description": "A focused VS Code extension that spins up a live MkDocs preview for intergrated doc browsing.",
  "version": "0.0.1",
  "publisher": "aswinunni01",
  "engines": {"vscode": "^1.109.0"},
  "categories": ["Development", "Productivity"],
  "main": "index.js",
  "activationEvents": ["workspaceContains:**/mkdocs.yml"],
